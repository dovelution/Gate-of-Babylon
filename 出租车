#include<stdio.h>
int main()
{
	int m=8,n;
	double s,min;
	scanf("%lf %lf",&s,&min);
	if(s<=3)
	{goto k;
	}
	else if(s<=10)
	{for(s=s-3;(s-1)>-1;s=s-1)
	 {m=m+2;
	 }
	}
	else if(s>10)
	{for(s=s-9,m=m+14;(s-1)>-1;s=s-1)
	 {m=m+3;
	 }
	}
	if(min>5)
	{min=min-5;
	 while((min=min-5)>-5)
	 {m=m+2;
	 }
	}
	k:printf("%d",m); 
	return 0;
}
